@model IEnumerable<adminconsole.Models.Locations>

@{
    ViewData["Title"] = "Index";
}

<div class="display-table-options">
    <div class="display-table-option">
        <input type="radio" name="tables" onclick=selectLocations() checked/> Locations
    </div>
    <div class="display-table-option">
        <input type="radio" name="tables" onclick=selectContacts() /> Contact
    </div>
    <div class="display-table-option">
        <input type="radio" name="tables" onclick=selectSpecialQualities() /> Special Qualities
    </div>
    <div class="display-table-option">
        <input type="radio" name="tables" onclick=selectDailyHours() /> Hours/Day of the Week
    </div>
    <div class="display-table-option">
        <input type="radio" name="tables" onclick=showAll() /> Show All

    </div>
</div>

<div class="showLocations">
    <table class="table table-bordered"
           id="table"
           data-toggle="table"
           data-search="true"
           data-show-search-clear-button="true"
           data-show-columns="true"
           data-show-columns-toggle-all="true"
           data-show-export="true"
           data-sort-stable="true"
           data-pagination="true">

        <thead>
            <tr>
                <th>
                    Controls
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LocationType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.City)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PostalCode)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.County)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.State)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Country)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Latitude)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Longitude)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RetailOutlet)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Hours)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.LocationId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.LocationId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.LocationId">Delete</a>
                </td>
                <td>
                    @if (item.Name.ToLower().Equals("becu"))
                    {
                        <img src="~/media/BEC-Logo-Icon-pms.png" />
                    }
                    else
                    {
                        <img src="~/media/logo_co-op.png" style="height: 40px;" />
                    }
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LocationType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Address)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PostalCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.County)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.State)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Country)
                </td>
                <td>
                    @item.Latitude
                </td>
                <td>
                    @item.Longitude
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RetailOutlet)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Hours)
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>


